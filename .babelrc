{
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@": "./",
          "underscore": "lodash"
        }
      }
    ],
    "babel-plugin-transform-typescript-metadata",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": false }]
  ],
  "presets": ["@babel/preset-env", "@babel/preset-typescript"]
}
